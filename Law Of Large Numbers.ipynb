    "import numpy as np\n",
    "from numpy.random import randn\n",
    "\n",
    "N = 10000\n",
    "count = 0\n",
    "for i in randn(10000):\n",
    "    if(i > -1 and i < 1):\n",
    "        count = count + 1\n",
    "answer = count / N\n",
    "print(answer)\n"
   ]
